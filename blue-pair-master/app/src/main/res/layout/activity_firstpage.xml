<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="co.aurasphere.bluepair.FirstPage">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        app:srcCompat="@drawable/title" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="152dp"
        android:layout_height="152dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="150dp"
        app:srcCompat="@drawable/blue" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="152dp"
        android:layout_height="152dp"
        android:layout_below="@id/ac"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="35dp"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="152dp"
        android:layout_height="152dp"
        android:layout_marginLeft="215dp"
        android:layout_marginTop="150dp"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="152dp"
        android:layout_height="152dp"
        android:layout_below="@id/projector"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/blue" />

    <ImageButton
        android:id="@+id/ac"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="160dp"
        android:background="@drawable/acnew" />

    <ImageButton
        android:id="@+id/projector"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="155dp"
        android:layout_marginRight="40dp"
        android:layout_toRightOf="@+id/ac"
        android:background="@drawable/projectornew2" />

    <ImageButton
        android:id="@+id/speaker"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="340dp"
        android:background="@drawable/speaker" />

    <ImageButton
        android:id="@+id/tv"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="340dp"
        android:layout_toRightOf="@id/speaker"
        android:background="@drawable/tvnew" />

</RelativeLayout>